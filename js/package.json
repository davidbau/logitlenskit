{
  "name": "logit-lens-widget",
  "version": "0.1.0",
  "description": "Interactive logit lens visualization widget for transformer models",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest tests/unit/",
    "test:integration": "jest tests/integration/ --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "esbuild src/logit-lens-widget.js --bundle --minify --outfile=dist/logit-lens-widget.min.js --global-name=LogitLensWidgetExport",
    "lint": "eslint src/ tests/",
    "callgraph": "node build/build-callgraph.js",
    "dataflow": "node build/build-dataflow.js"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "license": "MIT",
  "dependencies": {
    "acorn-loose": "^8.5.2"
  }
}
